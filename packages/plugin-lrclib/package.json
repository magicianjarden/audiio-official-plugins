{
  "name": "@audiio/plugin-lrclib",
  "version": "1.0.0",
  "description": "LRCLIB lyrics provider for Audiio - synced and plain lyrics",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "audiio": {
    "type": "plugin",
    "id": "lrclib",
    "roles": ["lyrics-provider"]
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@audiio/sdk": "^0.1.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "keywords": ["audiio", "plugin", "lrclib", "lyrics"],
  "author": "Audiio",
  "license": "MIT"
}
