{
  "name": "@audiio/plugin-sposify",
  "version": "1.0.0",
  "description": "Spotify data integration plugin for Audiio - import history, audio features, ISRC matching, and playlist discovery",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./ui": {
      "import": "./ui/index.ts",
      "types": "./ui/index.ts"
    }
  },
  "audiio": {
    "type": "plugin",
    "id": "sposify",
    "roles": ["tool"],
    "toolType": "data-transfer"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "fuse.js": "^7.0.0"
  },
  "peerDependencies": {
    "@audiio/core": "*",
    "electron": "*",
    "react": "*",
    "zustand": "*"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.17.12",
    "electron": "^33.4.0",
    "typescript": "^5.7.2"
  },
  "keywords": ["audiio", "plugin", "spotify", "import", "history"],
  "author": "Audiio",
  "license": "MIT"
}
