{
  "name": "@audiio/plugin-youtube-music",
  "version": "1.0.0",
  "description": "YouTube Music stream provider for Audiio - audio streaming and search",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "audiio": {
    "type": "plugin",
    "id": "youtube-music",
    "roles": ["stream-provider"]
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "youtubei.js": "^16.0.1"
  },
  "peerDependencies": {
    "@audiio/sdk": "^0.1.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "keywords": ["audiio", "plugin", "youtube", "music", "stream"],
  "author": "Audiio",
  "license": "MIT"
}
